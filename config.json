{
  "categories": ["NBA - Junior Events", "Pickleball"],
  "preEventQueryMinutes": 32,
  "fetchWindowHours": 168,
  "serviceRunIntervalHours": 1,
  "printMode": "email",
  "outputFilename": "attendees.pdf",
  "retry": {
    "maxAttempts": 3,
    "baseDelayMinutes": 5
  },
  "api": {
    "paginationLimit": 100,
    "paginationDelayMs": 1000,
    "cacheFreshSeconds": 120,
    "cacheStaleSeconds": 1800
  },
  "logging": {
    "maxFileSizeBytes": 5242880,
    "maxFiles": 5
  },
  "database": {
    "cleanupDays": 30
  },
  "pdfLayout": {
    "logo": "assets/logo.png",
    "fontSize": 10,
    "columns": [
      {
        "id": "name",
        "header": "Name",
        "width": 105
      },
      {
        "id": "phone",
        "header": "Phone",
        "width": 80
      },
      {
        "id": "signUpDate",
        "header": "Signed up",
        "width": 80
      },
      {
        "id": "fee",
        "header": "Fee",
        "width": 76
      },
      {
        "id": "status",
        "header": "Status",
        "width": 90
      }
    ]
  },
  "webhook": {
    "enabled": false,
    "url": null,
    "timeoutMs": 10000,
    "maxRetries": 2,
    "retryDelayMs": 2000
  },
  "service": {
    "gracePeriodMinutes": 60,
    "heartbeatIntervalMinutes": 15,
    "healthCheckIntervalSeconds": 60,
    "memoryHeapWarningMB": 300,
    "memoryRssWarningMB": 400
  }
}
