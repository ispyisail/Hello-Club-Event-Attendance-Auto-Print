<service>
	<id>helloclubeventattendance.exe</id>
	<name>HelloClubEventAttendance</name>
	<description>Automatically fetches and prints Hello Club event attendance lists</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>C:\Projects\Hello-Club-Event-Attendance-Auto-Print\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>C:\Projects\Hello-Club-Event-Attendance-Auto-Print\src\index.js</argument>
	<argument>--scriptoptions=start-service</argument>
	<argument>--log</argument>
	<argument>HelloClubEventAttendance wrapper</argument>
	<argument>--grow</argument>
	<argument>0.5</argument>
	<argument>--wait</argument>
	<argument>2</argument>
	<argument>--maxrestarts</argument>
	<argument>10</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<argument>--maxretries</argument>
	<argument>5</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<env name="NODE_ENV" value="production"/>
	<serviceaccount>
		<domain>MATARAU</domain>
		<user>LocalSystem</user>
		<password></password>
		<allowservicelogon>true</allowservicelogon>
	</serviceaccount>
	<workingdirectory>C:\Projects\Hello-Club-Event-Attendance-Auto-Print</workingdirectory>
</service>